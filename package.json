{
  "name": "epstein-archive",
  "private": true,
  "version": "4.2.0",
  "description": "World-class Investigation Workspace UI with accessibility-first, consistent design and decluttered layouts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "api": "tsx src/server.ts",
    "api:prod": "NODE_ENV=production tsx src/server.production.ts",
    "dev:api": "concurrently \"npm run api\" \"npm run dev\"",
    "start": "npm run api:prod",
    "start:full": "concurrently \"npm run api:prod\" \"npm run preview\"",
    "migrate": "tsx src/scripts/migrateData.ts",
    "migrate:full": "tsx src/scripts/fullMigrate.ts",
    "migrate:complete": "tsx src/scripts/completeDatabaseMigration.ts",
    "migrate:evidence-types": "tsx src/scripts/populateEvidenceTypes.ts",
    "migrate:verify": "tsx -e \"import { dataMigrator } from './src/scripts/migrateData'; dataMigrator.verifyMigration().then(console.log)\"",
    "populate:media": "tsx scripts/populateMediaItems.ts",
    "media:normalize": "tsx src/scripts/normalizeMediaPaths.ts",
    "db:test": "tsx src/scripts/testDatabaseSchema.ts",
    "db:stats": "tsx -e \"import { databaseService } from './src/services/DatabaseService'; databaseService.getStatistics().then(console.log)\"",
    "import:known": "tsx src/scripts/importKnownEntities.ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:prod": "NODE_ENV=production playwright test",
    "clean": "./cleanup.sh",
    "build:deploy": "./build-and-deploy.sh",
    "reindex:forensic": "tsx src/scripts/reindexForensicMetadata.ts",
    "enrich:external": "EXTERNAL_DATA_ROOT=\"/Users/veland/Downloads/Epstein Files/data\" tsx scripts/enrich_external_data.ts",
    "sync:external:data": "EXTERNAL_DATA_ROOT=\"/Users/veland/Downloads/Epstein Files/data\" tsx scripts/sync_external_json.ts",
    "ingest:fs": "DATA_ROOT=\"/Users/veland/Downloads/Epstein Files/data\" tsx src/scripts/ingest_from_fs.ts",
    "db:rebuild": "tsx src/scripts/rebuild_database.ts",
    "relationships:compute": "tsx scripts/compute_relationships_weighted.ts"
  },
