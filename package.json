{
  "name": "epstein-archive",
  "private": true,
  "version": "12.5.0",
  "description": "Epstein Archive Analyzer",
  "type": "module",
  "scripts": {
    "dev": "DB_PATH=./epstein-archive.db vite",
    "build": "vite build",
    "build:prod": "vite build && tsc -p tsconfig.server.json",
    "start": "NODE_ENV=production DB_PATH=./epstein-archive.db RAW_CORPUS_BASE_PATH=./data node dist/server.js",
    "server": "DB_PATH=./epstein-archive.db tsx src/server.ts",
    "api": "DB_PATH=./epstein-archive.db tsx src/server.ts",
    "preview": "vite preview",
    "migrate": "DB_PATH=./epstein-archive.db tsx scripts/migrate.ts",
    "seed:structure": "DB_PATH=./epstein-archive.db tsx scripts/ensure_structure.ts",
    "verify": "DB_PATH=./epstein-archive.db tsx scripts/verify_deployment.ts",
    "verify:post-deploy": "./scripts/post_deploy_verify.sh",
    "rollback": "./scripts/emergency_rollback.sh",
    "seed:sascha-investigation": "DB_PATH=./epstein-archive.db tsx scripts/create_sascha_investigation.ts",
    "seed:katie-videos": "DB_PATH=./epstein-archive.db KJ_VIDEO_ROOT=./data/media/videos/KatieJohnson tsx scripts/ingest_katie_videos.ts",
    "db:merge": "tsx scripts/sync-db.ts",
    "db:push": "./deploy.sh --db-only",
    "db:pull": "./scripts/sync_down_safe.sh",
    "db:optimize": "tsx scripts/maintenance.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test:smoke": "tsx scripts/smoke-tests.ts",
    "deploy:prod": "./deploy.sh",
    "prepare": "husky"
  },
